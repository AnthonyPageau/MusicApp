<h1>Users</h1>

<%= link_to 'New User', new_user_path, class: "button" %>

<table>
  <thead>
    <tr>
      <th>id</th>
      <th>name</th>
    </tr>
  </thead>
  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.id %></td>
        <td><%= user.username %></td>
      </tr>
    <% end %>
  </tbody>
</table>
