<%= link_to 'New User', new_user_path, class: "button" %>

<table>
  <thead>
    <tr>
      <th>Username</th>
    </tr>
  </thead>
  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= link_to user.username, user_path(user),style: 'text-decoration: none; color: black;'  %></td>
      </tr>
    <% end %>
  </tbody>
</table>
